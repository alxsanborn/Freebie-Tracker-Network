<h1>New Freebie!</h1>

  <%= form_for @item do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br>
    <%= f.label :market_value %>
    <%= f.text_field :market_value %> <br>
    <%= f.collection_select :brand_id, Brand.all, :id, :name %><br>
    <%= f.file_field :image %> <br><br>
    <%= f.submit "Add your freebie!"%>
  <% end %>
